{
    "name": "fisca/yii2_advanced_test",
    "description": "Learn and test Yii 2 Advanced Application Template",
    "keywords": ["yii2", "framework", "advanced", "application template"],
    "homepage": "http://localhost/yii2_advanced_test/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/fisca/yii2_advanced_test/issues?state=open",
        "forum": "https://github.com/fisca/yii2_advanced_test/forum/",
        "wiki": "https://github.com/fisca/yii2_advanced_test/wiki/",
        "irc": "https://github.com/fisca/yii2_advanced_test/irc/",
        "source": "https://github.com/fisca/yii2_advanced_test"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
